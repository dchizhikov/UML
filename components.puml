@startuml
!procedure $components($names, $arg)
  !while $arg!=0
    !$arg1=$arg - 1
    !$name=$names.name[$arg1]
    !$ps_name = "comp_"+$arg
    [$name] as $ps_name <<subsystem>>
    !$arg = $arg - 1
  !endwhile
!endprocedure

!procedure $interfaces($names, $arg)
  !while $arg!=0
    !$arg1=$arg - 1
    !$name=$names.name[$arg1]
    !$ps_name = "iF_"+$arg
    ($name) as $ps_name
    !$arg = $arg - 1
  !endwhile
!endprocedure
@enduml
